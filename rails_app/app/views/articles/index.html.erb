<h1> Listing Articles </h1>
<table>
    <tr>
        <td>Title</td>
        <td>Text</td>
    </tr>

    <% @articles.each do |a| %>
        <tr>
            <td><%= a.title %></td>
            <td><%= a.text %></td>
        </tr>
    <% end %>
</table>
<%= link_to "My Blog",controller: 'articles' %>
